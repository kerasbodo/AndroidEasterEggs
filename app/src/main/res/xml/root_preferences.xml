<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.dede.android_eggs.EggCollection
        app:iconSpaceReserved="false"
        app:key="key_collection"
        app:title="@string/header_collection">

        <Preference app:title="MagicSushi">
            <intent
                android:targetClass="com.dede.android_eggs.MagicSushiActivity"
                android:targetPackage="com.dede.android_eggs" />
        </Preference>

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_t"
            app:icon="@drawable/t_platlogo"
            app:iconPadding="1.5dp"
            app:summary="@string/t_egg_name"
            app:title="@string/title_android_t" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_s"
            app:icon="@drawable/s_icon"
            app:iconPadding="1.5dp"
            app:summary="@string/s_egg_name"
            app:title="@string/title_android_s" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_r"
            app:icon="@drawable/r_icon"
            app:iconPadding="1.5dp"
            app:summary="@string/r_egg_name"
            app:supportAdaptiveIcon="oval"
            app:title="@string/title_android_r" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_q"
            app:icon="@drawable/q_icon"
            app:iconPadding="1.5dp"
            app:summary="@string/q_egg_name"
            app:supportAdaptiveIcon="oval"
            app:title="@string/title_android_q" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_p"
            app:icon="@drawable/p_icon"
            app:iconPadding="1.5dp"
            app:summary="@string/p_app_name"
            app:supportAdaptiveIcon="oval"
            app:title="@string/title_android_p" />

        <com.dede.android_eggs.EggPreference
            app:icon="@drawable/o_icon"
            app:iconPadding="-2dp"
            app:summary="@string/o_app_name"
            app:title="@string/title_android_o_point">
            <intent
                android:targetClass="com.android_o.egg.PlatLogoActivity"
                android:targetPackage="com.dede.android_eggs">
                <extra
                    android:name="isOreoPoint"
                    android:value="true" />
            </intent>
        </com.dede.android_eggs.EggPreference>

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_o"
            app:icon="@drawable/o_icon"
            app:iconPadding="-2dp"
            app:summary="@string/o_app_name"
            app:title="@string/title_android_o" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_n"
            app:icon="@drawable/n_icon"
            app:summary="@string/n_app_name"
            app:title="@string/title_android_n" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_m"
            app:icon="@drawable/m_icon"
            app:summary="@string/m_mland"
            app:title="@string/title_android_m" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_l"
            app:icon="@drawable/l_icon"
            app:summary="@string/l_lland"
            app:title="@string/title_android_l" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_k"
            app:icon="@drawable/ic_k_platlogo"
            app:summary="@string/k_dessert_case"
            app:title="@string/title_android_k" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_j"
            app:icon="@drawable/j_platlogo"
            app:summary="@string/j_egg_name"
            app:title="@string/title_android_j" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_i"
            app:icon="@drawable/i_platlogo"
            app:iconPadding="2.5dp"
            app:summary="@string/i_egg_name"
            app:title="@string/title_android_i" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_h"
            app:icon="@drawable/h_platlogo"
            app:summary="@string/h_egg_name"
            app:title="@string/title_android_h" />

        <com.dede.android_eggs.EggPreference
            android:targetClass="@string/target_class_android_g"
            app:icon="@drawable/g_platlogo"
            app:summary="@string/g_egg_name"
            app:title="@string/title_android_g" />

    </com.dede.android_eggs.EggCollection>

    <PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="@string/header_about">

        <com.dede.android_eggs.ChromeTabPreference
            app:customUrl="@string/url_github"
            app:icon="@drawable/ic_logo_github"
            app:summary="@string/summary_github"
            app:title="@string/title_github" />

        <com.dede.android_eggs.ChromeTabPreference
            app:customUrl="@string/url_source"
            app:icon="@drawable/ic_android_cs"
            app:summary="@string/summary_source"
            app:title="@string/title_source" />

        <com.dede.android_eggs.VersionPreference
            app:customUrl="@string/url_beta"
            app:icon="@drawable/ic_logo_android"
            app:title="@string/title_version" />

    </PreferenceCategory>

</PreferenceScreen>